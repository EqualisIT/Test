rm(list = ls())     # clear objects  
graphics.off()      # close graphics windows 
#options(dec = ",")
if(!require(log4r)) {
  install.packages("log4r"); require(log4r)}
if(!require(RODBC)) {
  install.packages("RODBC"); require(RODBC)}
if(!require(ggplot2)) {
  install.packages("ggplot2"); require(ggplot2)}
if(!require(grid)) {
  install.packages("grid"); require(grid)}
if(!require(png)) {
  install.packages("png"); require(png)}
if(!require(Cairo)) {
  install.packages("Cairo"); require(Cairo)}
if(!require(reshape)) {
  install.packages("reshape"); require(reshape)}
if(!require(rJava)) {
  install.packages("rJava"); require(rJava)}
library(RODBC)
